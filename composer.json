{
    "name": "sunny5156/hyperf-socialite",
    "description": "socialite ",
    "license": "MIT",
    "authors": [
        {
            "name": "sunny5156",
            "email": "sunny5156@qq.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "ext-json": "*",
        "hyperf/http-message": "^3.0",
        "hyperf/guzzle": "^3.0",
        "hyperf/contract": "^3.0",
        "hyperf/http-server": "^3.0",
        "league/oauth1-client": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Cblink\\Hyperf\\Socialite\\": "src/",
            "HyperfSocialiteProviders\\": "providers/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Cblink\\Hyperf\\Socialite\\ConfigProvider"
        }
    },
    "scripts": {
        "fix": "./vendor/bin/php-cs-fixer fix --using-cache=no --config=.php_cs --ansi"
    }
}
